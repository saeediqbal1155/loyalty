# Ignore the CodeIgniter application cache
application/cache/*
!application/cache/index.html

# Ignore the CodeIgniter application logs
application/logs/*
!application/logs/index.html

# Ignore the CodeIgniter application config files
application/config/config.php
application/config/database.php

# Ignore the CodeIgniter session files
application/sessions/*

# Ignore the CodeIgniter vendor directory (if using Composer)
vendor/

# Ignore Composer dependencies
composer.phar
composer.lock

# Ignore the node_modules directory (if using Node.js)
node_modules/

# Ignore the public uploads directory
public/uploads/*

# Ignore IDE specific files
../.idea/
*.sublime-project
*.sublime-workspace

# Ignore OS generated files
.DS_Store
Thumbs.db

# Ignore PHPStorm project files
*.iml
*.iws
.idea/

# Ignore PHPUnit test results
phpunit.xml
phpunit.xml.dist

# Ignore environment configuration files
.env
.env.*

# Ignore backup files
*.bak
*.backup
*.swp

# Ignore other unnecessary files
*.log
*.cache
*.tmp